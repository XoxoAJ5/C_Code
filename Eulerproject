#include <iostream>
#include <cmath>

using namespace std;

int main() {
    int count = 0;
    int sum = 0;
    while (count != 526000) {
        for (int i = 0; i < pow(10,100); i++) {
            if (i % 2 == 1) {
                count += 1;
                sum += pow(i, 2);
        }
    }
    cout << sum;
    return 0;
}
}